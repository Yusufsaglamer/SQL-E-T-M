--Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.
update employee Set name='Mehmet' where name='Horatio' returning *;
update employee set birthday='1980-05-22' where email='iskilbeck1b@dot.gov'returning *;
update employee set name='halil' where id=2 returning *;
update employee set email='gs_1147@gmail.com' where name='Dion' returning *;
update employee set id=1905 where id=4 returning *;
select * from employee;
--Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM employee WHERE id = 1905 RETURNING *;
DELETE FROM employee WHERE name='Mehmet' RETURNING *;
DELETE FROM employee WHERE email='gs_1147@gmail.com' RETURNING *;
DELETE FROM employee WHERE birthday='1980-05-22' RETURNING *;
DELETE FROM employee WHERE id between 40 and 44  RETURNING *;
select * from employe
